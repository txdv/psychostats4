<?xml version='1.0' encoding='utf-8' ?>
<images>
	<ccimg width="800" height="300" antialias="0">
		<frame color="gray" margin="#d7d7d7" width="1">
			<title font="FF_FONT1">Breakdown of Countries</title>
		</frame>
		<slices border="0" total="10" theme="earth" />
	</ccimg>
	<connimg width="800" height="250">
		<frame color="gray" margin="#d7d7d7" width="1">
			<title font="FF_FONT2">Connections Per Day</title>
			<ygrid show="1" color1="whitesmoke" color2="azure2" />
			<bar color1="#3658F5" color2="#030C36" type="GRAD_HOR">Maximum</bar>
			<plot color="blue@0.90" weight="1">Maximum</plot>
			<plot color="khaki4" weight="2">Average</plot>
		</frame>
		<legend x="10" y="10" halign="right" valign="top" color="lightblue@0.5" font="FF_FONT1">
			<shadow color="gray@0.5" width="2" />
		</legend>
	</connimg>
	<sessimg>
		<frame margin="#C4C4C4" color="gray" width="0">
			<title>Player Sessions</title>
			<header bgcolor="lightyellow:1.5" />
			<hgrid show="1" color1="gray@0.7" color2="#E4DCCE@0.6" />
			<bar patternfill="blue@0.9" fill="blue@0.9" pattern="BAND_SOLID" showempty="0" />
		</frame>
	</sessimg>
	<quickimg width="287" height="180" interval="3000">
		<frame width="1" color="gray" margin="#EEEEEE">
			<title font="FF_FONT1">Quick History</title>
			<ygrid show="1" color1="whitesmoke" color2="azure2" />
			<xgrid show="1" />
			<plot>
				<color>blue@0.90</color>
				<weight>1</weight>
				<title>Total</title>
			</plot>
			<plot>
				<color>khaki4</color>
				<weight>2</weight>
				<title>Average</title>
			</plot>
		</frame>
		<legend x="5" y="5" halign="right" valign="top" color="lightyellow@0.5" font="FF_FONT0">
			<shadow color="gray@0.5" width="2" />
		</legend>
	</quickimg>

	<common>
		<footer show="1" color="#999999" font="FF_FONT0">
			<left>PsychoStats v%s</left>
			<right>M d @ H:i:s</right>
		</footer>
	</common>
</images>
